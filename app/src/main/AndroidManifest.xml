<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.woo.qlsp_trungvit">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MuaVao"
            android:label="@string/app_mua"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BanRa"
            android:label="Danh Sách Bán Ra"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".KhachHang"
            android:label="@string/app_khachhang"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ThongKe"
            android:label="@string/app_thongke"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AddKhachHang"
            android:label="@string/app_addkhachhang"
            android:parentActivityName=".KhachHang" />
        <activity
            android:name=".AddGiaoDich"
            android:label="@string/app_addgiaodich"
            android:parentActivityName=".MuaVao" />
        <activity
            android:name=".Details"
            android:label="Chi tiết sản phẩm" />
        <activity
            android:name=".EditGiaoDich"
            android:label="Sửa giao dịch">

        </activity>
    </application>

</manifest>